#include <stdio.h>
int main(void){
    int size;
    printf("Enter size: ");
    scanf("%d", &size);
    
    int length = size;
    int height = size;
    int counter = 0;
    int counter2 = counter;
    
    
    while (length >= counter){
        printf("*");
            while (length-1 >= counter2){
            printf("_");
            counter2 = counter2 + 1;
            }
        printf("*");
    counter = counter + 1;
    }
}
